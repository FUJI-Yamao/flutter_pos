drop table if exists c_crdt_actual_log cascade;
DROP SEQUENCE if exists c_crdt_actual_log_s0;
CREATE SEQUENCE c_crdt_actual_log_s0 cycle;
CREATE TABLE c_crdt_actual_log (
  serial_no             int4             	default nextval('c_crdt_actual_log_s0'),
  crdt_flg              char,
  stre_cd               numeric(6),
  mac_no                numeric(6),
  receipt_no            numeric(4),
  print_no              numeric(4),
  chkr_no               numeric(6),
  cshr_no               numeric(6),
  now_sale_datetime     timestamp,
  sale_date             timestamp,
  ope_mode_flg          smallint,
  crdt_no               numeric(4),
  data_stat             char,
  data_div              char,
  ttl_lvl               char,
  tran_div              numeric(2),
  tran_cd               char,
  demand_1st_yymm       numeric(4),
  divide_cnt            numeric(3),
  stre_join_no          char(15),
  mbr_cd                char(16),
  sale_yymmdd           numeric(6),
  sale_price            int4,
  tax_postage           int4,
  sale_amt              int4,
  recogn_no             numeric(6),
  good_thru             numeric(4),
  filler                char(6),
  bonus_month_sign      char,
  bonus_cnt             char,
  bonus_amt             int4,
  bonus_1st_yymm        numeric(4),
  pos_recogn_no         char(13),
  pos_receipt_no        numeric(5),
  item_cd               numeric(3),
  space                 char(3),
  cha_cnt1              int4,
  cha_amt1              int4,
  cha_cnt2              int4,
  cha_amt2              int4,
  cha_cnt3              int4,
  cha_amt3              int4,
  cha_cnt4              int4,
  cha_amt4              int4,
  cha_cnt5              int4,
  cha_amt5              int4,
  cha_cnt6              int4,
  cha_amt6              int4,
  cha_cnt7              int4,
  cha_amt7              int4,
  cha_cnt8              int4,
  cha_amt8              int4,
  cha_cnt9              int4,
  cha_amt9              int4,
  cha_cnt10             int4,
  cha_amt10             int4,
  cncl_reason           char,
  sell_sts              int4,
  sell_kind             int4,
  sale_kind             char,
  seq_inq_no            numeric(13),
  mng_pos_no            numeric(13),
  seq_pos_no            numeric(13),
  tenant_cd             numeric(8),
  div_com               int4,
  judg_cd               char(2),
  black_check           char,
  card_comp_cd          numeric(5),
  change_chk_no         numeric(10),
  cncl_slip_no          numeric(11),
  sign                  char,
  card_stre_cd          numeric(6),
  req_code              char(4),
  card_jis1             char(37),
  card_jis2             char(69),
  handle_div            char(3),
  pay_a_way             char(81),
  void_mac_no           numeric(6),
  void_receipt_no       numeric(4),
  void_print_no         numeric(4),
  person_cd             numeric(6),
  PRIMARY KEY (serial_no)
);

