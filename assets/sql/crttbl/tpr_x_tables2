
drop table if exists c_smlsch_mst cascade;
CREATE TABLE c_smlsch_mst (
  stre_cd               numeric(6),
  smlsch_cd             numeric(3),
  plan_cd               numeric(9),
  name                  varchar(50),
  short_name            varchar(10),
  start_datetime        timestamp,
  end_datetime          timestamp,
  timesch_flg           smallint,
  sun_flg               smallint,
  mon_flg               smallint,
  tue_flg               smallint,
  wed_flg               smallint,
  thu_flg               smallint,
  fri_flg               smallint,
  sat_flg               smallint,
  poppy_flg             smallint,
  stop_flg              smallint,
  trends_typ            smallint,
  ins_datetime          timestamp,
  upd_datetime          timestamp,
  status                smallint,
  send_flg              smallint,
  upd_user              numeric(6),
  upd_system            smallint,
  upd_machin            numeric(6),
  efct_flg              smallint,
  PRIMARY KEY (stre_cd,smlsch_cd)
);

drop table if exists c_smlitem_mst cascade;
CREATE TABLE c_smlitem_mst (
  stre_cd               numeric(6),
  smlcls_cd             numeric(6),
  smlsch_cd             numeric(3),
  showorder             smallint,
  poppy_flg             smallint,
  stop_flg              smallint,
  dsc_flg               smallint,
  dsc_prc               numeric(7),
  ins_datetime          timestamp,
  upd_datetime          timestamp,
  status                smallint,
  send_flg              smallint,
  upd_user              numeric(6),
  upd_system            smallint,
  upd_machin            numeric(6),
  mdsc_prc              numeric(7)       	default '0',
  PRIMARY KEY (stre_cd,smlcls_cd,smlsch_cd)
);

