  upd_system            smallint,
  upd_machin            numeric(6),
  brgn_dsc              numeric(7),
  mbrbrgn_dsc           numeric(7),
  brgn_div              smallint,
  brgn_costper          numeric(9,2),
  ts_fil_cd1            numeric(10),
  ts_fil_cd2            numeric(10),
  ts_fil_cd3            numeric(10),
  ts_fil_cd4            numeric(10),
  ts_fil_cd5            numeric(10),
