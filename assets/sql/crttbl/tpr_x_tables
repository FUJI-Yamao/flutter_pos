  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , constraint c_divide2_mst_PKC primary key (comp_cd,stre_cd,kind_cd,div_cd)
);

-- ハイタッチ受信ログ
drop table if exists c_hitouch_rcv_log cascade;

CREATE TABLE c_hitouch_rcv_log (
  rcv_datetime TIMESTAMP
  , tag_id VARCHAR(10)
  , plu_cd VARCHAR(20)
  , upd_flg SMALLINT default 0 not null
  , constraint c_hitouch_rcv_log_PKC primary key (rcv_datetime, tag_id)
) ;

-- index 追加の際は, src/vup/db/vup_db3.c の vup_db_proc_create_index() を参照すること. 記述ルールがあります.

DROP INDEX IF EXISTS p_promsch_mst_idx;
CREATE INDEX p_promsch_mst_idx ON p_promsch_mst (start_datetime, end_datetime, stop_flg, sch_typ, item_cd, timesch_flg);

DROP INDEX IF EXISTS p_promitem_mst_idx;
CREATE INDEX p_promitem_mst_idx ON p_promitem_mst (prom_typ);

