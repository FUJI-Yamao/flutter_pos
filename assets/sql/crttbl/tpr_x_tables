  , cshr_name VARCHAR(25)
  , cshr_status SMALLINT default 0 not null
  , cshr_open_time TIMESTAMP
  , cshr_start_no NUMERIC(9) default 0 not null
  , cshr_end_no NUMERIC(9) default 0 not null
  , ins_datetime TIMESTAMP
  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , constraint c_staffopen_mst_PKC primary key (comp_cd,stre_cd,mac_no)
) ;


-- 10
-- 履歴ログ
drop table if exists c_histlog_mst cascade;

drop sequence if exists c_histlog_mst_s0;
create sequence c_histlog_mst_s0 cycle;

create table c_histlog_mst (
  hist_cd NUMERIC(18) default nextval('c_histlog_mst_s0')
  , ins_datetime TIMESTAMP
  , comp_cd NUMERIC(9) default 0 not null
  , stre_cd NUMERIC(9) default 0 not null
  , table_name VARCHAR(50)
  , mode SMALLINT default 0 not null
  , mac_flg numeric(3)
  , data1 VARCHAR(8000)
  , data2 VARCHAR(8000)
  , constraint c_histlog_mst_PKC primary key (hist_cd)
) ;

-- レジ履歴ログカウント
drop table if exists c_histlog_chg_cnt cascade;

create table c_histlog_chg_cnt (
  counter_cd NUMERIC(9)
  , hist_cd NUMERIC(18)
  , ins_datetime TIMESTAMP
  , constraint c_histlog_chg_cnt_PKC primary key (counter_cd)
) ;

-- 履歴ログ制御コントロールマスタ
drop table if exists hist_ctrl_mst cascade;

create table hist_ctrl_mst (
  ctrl_cd numeric(6)
  , flg1 SMALLINT
  , flg2 SMALLINT
  , flg3 SMALLINT
  , flg4 SMALLINT
  , flg5 SMALLINT
  , flg6 SMALLINT
  , flg7 SMALLINT
  , flg8 SMALLINT
  , flg9 SMALLINT
  , flg10 SMALLINT
  , constraint hist_ctrl_mst_PKC primary key (ctrl_cd)
) ;

-- 履歴ログスキップコード番号保持テーブル
drop table if exists histlog_skip_num cascade;

create table histlog_skip_num(
	hist_cd NUMERIC(18)
);

