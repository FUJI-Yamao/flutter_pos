  , s_custom2 SMALLINT default 0 not null
  , s_custom3 SMALLINT default 0 not null
  , s_custom4 SMALLINT default 0 not null
  , s_custom5 SMALLINT default 0 not null
  , s_custom6 SMALLINT default 0 not null
  , s_custom7 SMALLINT default 0 not null
  , s_custom8 SMALLINT default 0 not null
  , c_custom1 VARCHAR(50)
  , c_custom2 VARCHAR(50)
  , c_custom3 VARCHAR(50)
  , c_custom4 VARCHAR(50)
  , d_custom1 TIMESTAMP
  , d_custom2 TIMESTAMP
  , d_custom3 TIMESTAMP
  , d_custom4 TIMESTAMP
  , constraint c_cpnhdr_mst_PKC primary key (cpn_id,comp_cd,stre_cd)
) ;

