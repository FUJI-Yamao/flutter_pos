-- RDBMS Type   : PostgreSQL

-- 01
-- 企業マスタ
drop table if exists c_comp_mst cascade;

create table c_comp_mst (
  comp_cd NUMERIC(9)
  , comp_typ SMALLINT default 0 not null
  , rtr_id NUMERIC(9) default 0 not null
  , name VARCHAR(25)
  , short_name VARCHAR(5)
  , kana_name VARCHAR(25)
  , post_no VARCHAR(7)
  , adress1 VARCHAR(50)
  , adress2 VARCHAR(50)
  , adress3 VARCHAR(50)
  , telno1 VARCHAR(25)
  , telno2 VARCHAR(25)
  , srch_telno1 VARCHAR(20)
  , srch_telno2 VARCHAR(20)
  , ins_datetime TIMESTAMP
  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , constraint c_comp_mst_PKC primary key (comp_cd)
) ;

-- 店舗マスタ
drop table if exists c_stre_mst cascade;

create table c_stre_mst (
  stre_cd NUMERIC(9)
  , comp_cd NUMERIC(9)
  , zone_cd NUMERIC(9) default 0 not null
  , bsct_cd NUMERIC(9) default 0 not null
  , name VARCHAR(25)
  , short_name VARCHAR(5)
  , kana_name VARCHAR(25)
  , post_no VARCHAR(7)
  , adress1 VARCHAR(50)
  , adress2 VARCHAR(50)
  , adress3 VARCHAR(50)
  , telno1 VARCHAR(25)
  , telno2 VARCHAR(25)
  , srch_telno1 VARCHAR(20)
  , srch_telno2 VARCHAR(20)
  , ip_addr VARCHAR(50)
  , trends_typ SMALLINT default 0 not null
  , stre_typ SMALLINT default 0 not null
  , flg_shp SMALLINT default 0 not null
  , business_typ1 SMALLINT default 0 not null
  , business_typ2 SMALLINT default 0 not null
  , chain_other_flg SMALLINT default 0 not null
  , locate_typ SMALLINT default 0 not null
  , openclose_flg SMALLINT default 0 not null
  , opentime TIMESTAMP
  , closetime TIMESTAMP
  , floorspace NUMERIC(7,2) default 0 not null
  , today TIMESTAMP
  , bfrday TIMESTAMP
  , twodaybfr TIMESTAMP
  , nextday TIMESTAMP
  , sysflg_base SMALLINT default 0 not null
  , sysflg_sale SMALLINT default 0 not null
  , sysflg_purchs SMALLINT default 0 not null
  , sysflg_order SMALLINT default 0 not null
  , sysflg_invtry SMALLINT default 0 not null
  , sysflg_cust SMALLINT default 0 not null
  , sysflg_poppy SMALLINT default 0 not null
  , sysflg_elslbl SMALLINT default 0 not null
  , sysflg_fresh SMALLINT default 0 not null
  , sysflg_wdslbl SMALLINT default 0 not null
  , sysflg_24hour SMALLINT default 0 not null
  , showorder SMALLINT default 0 not null
  , opendate TIMESTAMP
  , stre_ver_flg SMALLINT default 0 not null
  , sunday_off_flg SMALLINT default 0 not null
  , monday_off_flg SMALLINT default 0 not null
  , tuesday_off_flg SMALLINT default 0 not null
  , wednesday_off_flg SMALLINT default 0 not null
  , thursday_off_flg SMALLINT default 0 not null
  , friday_off_flg SMALLINT default 0 not null
  , saturday_off_flg SMALLINT default 0 not null
  , itemstock_flg SMALLINT default 0 not null
  , wait_time VARCHAR(4)
  , ins_datetime TIMESTAMP
  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , entry_no VARCHAR(32)
  , constraint c_stre_mst_PKC primary key (stre_cd,comp_cd)
) ;

-- 店舗通信マスタ
drop table if exists c_connect_mst cascade;

create table c_connect_mst (
  comp_cd NUMERIC(9)
  , stre_cd NUMERIC(9)
  , connect_cd NUMERIC(9)
  , connect_typ SMALLINT default 0 not null
  , name VARCHAR(25)
  , short_name VARCHAR(5)
  , kana_name VARCHAR(25)
  , host_name VARCHAR(25)
  , opentime TIMESTAMP
  , closetime TIMESTAMP
  , format_typ SMALLINT default 0 not null
  , network_typ SMALLINT default 0 not null
  , telno1 VARCHAR(25)
  , telno2 VARCHAR(25)
  , srch_telno1 VARCHAR(20)
  , srch_telno2 VARCHAR(20)
  , ip_addr VARCHAR(50)
  , retry_cnt SMALLINT default 0 not null
  , retry_time SMALLINT default 0 not null
  , time_out SMALLINT default 0 not null
  , ftp_put_dir varchar(100)
  , ftp_get_dir varchar(100)
  , connect_time1 SMALLINT default 0 not null
  , connect_time2 SMALLINT default 0 not null
  , cnt_usr VARCHAR(20)
  , cnt_pwd VARCHAR(20)
  , wait_time1 SMALLINT default 0 not null
  , wait_time2 SMALLINT default 0 not null
  , cnt_interval1 SMALLINT default 0 not null
  , cnt_interval2 SMALLINT default 0 not null
  , stre_chk_dgt SMALLINT default 0 not null
  , ins_datetime TIMESTAMP
  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , constraint c_connect_mst_PKC primary key (comp_cd,stre_cd,connect_cd)
) ;

-- 分類マスタ
drop table if exists c_cls_mst cascade;

create table c_cls_mst (
  comp_cd NUMERIC(9)
  , stre_cd NUMERIC(9)
  , cls_typ SMALLINT
  , lrgcls_cd NUMERIC(9)
  , mdlcls_cd NUMERIC(9)
  , smlcls_cd NUMERIC(9)
  , tnycls_cd NUMERIC(9)
  , plu_cd VARCHAR(20)
  , cls_flg SMALLINT default 0 not null
  , tax_cd1 SMALLINT default 0 not null
  , tax_cd2 SMALLINT default 0 not null
  , tax_cd3 SMALLINT default 0 not null
  , tax_cd4 SMALLINT default 0 not null
  , name VARCHAR(25)
  , short_name VARCHAR(5)
  , kana_name VARCHAR(25)
  , margin_flg SMALLINT default 0 not null
  , regsale_flg SMALLINT default 0 not null
  , clothdeal_flg SMALLINT default 0 not null
  , dfltcls_cd NUMERIC(9) default 0 not null
  , msg_name VARCHAR(25)
  , pop_msg VARCHAR(25)
  , nonact_flg SMALLINT default 0 not null
  , max_prc NUMERIC(7) default 0 not null
  , min_prc NUMERIC(7) default 0 not null
  , cost_per NUMERIC(5,2) default 0 not null
  , loss_per NUMERIC(5,2) default 0 not null
  , rbtpremium_per NUMERIC(5,2) default 1.00 not null
  , prc_chg_flg SMALLINT default 0 not null
  , rbttarget_flg SMALLINT default 0 not null
  , stl_dsc_flg SMALLINT default 0 not null
  , labeldept_cd NUMERIC(9) default 0 not null
  , multprc_flg SMALLINT default 0 not null
  , multprc_per NUMERIC(5,2) default 0 not null
  , sch_flg SMALLINT default 0 not null
  , stlplus_flg SMALLINT default 0 not null
  , pctr_tckt_flg SMALLINT default 0 not null
  , clothing_flg SMALLINT default 0 not null
  , spclsdsc_flg SMALLINT default 0 not null
  , bdl_dsc_flg SMALLINT default 0 not null
  , self_alert_flg SMALLINT default 0 not null
  , chg_ckt_flg SMALLINT default 0 not null
  , self_weight_flg SMALLINT default 0 not null
  , msg_flg SMALLINT default 0 not null
  , pop_msg_flg SMALLINT default 0 not null
  , itemstock_flg SMALLINT default 0 not null
  , orderpatrn_flg SMALLINT default 0 not null
  , orderbook_flg SMALLINT default 0 not null
  , safestock_per NUMERIC(5,2) default 0 not null
  , autoorder_typ SMALLINT default 0 not null
  , casecntup_typ SMALLINT default 0 not null
  , producer_cd NUMERIC(9) default 0 not null
  , cust_dtl_flg SMALLINT default 0 not null
  , coupon_flg SMALLINT default 0 not null
  , kitchen_prn_flg SMALLINT default 0 not null
  , pricing_flg SMALLINT default 0 not null
  , user_val_1 NUMERIC(9) default 0 not null
  , user_val_2 NUMERIC(9) default 0 not null
  , user_val_3 NUMERIC(9) default 0 not null
  , user_val_4 NUMERIC(9) default 0 not null
  , user_val_5 NUMERIC(9) default 0 not null
  , ins_datetime TIMESTAMP
  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , tax_exemption_flg SMALLINT default 0 not null
  , dpnt_rbttarget_flg SMALLINT default 0 not null
  , dpnt_usetarget_flg SMALLINT default 0 not null
  , constraint c_cls_mst_PKC primary key (comp_cd,stre_cd,cls_typ,lrgcls_cd,mdlcls_cd,smlcls_cd,tnycls_cd)
) ;

-- 分類グループマスタ
drop table if exists c_grp_mst cascade;

create table c_grp_mst (
  comp_cd NUMERIC(9)
  , stre_cd NUMERIC(9)
  , cls_grp_cd NUMERIC(9)
  , mdl_smlcls_cd NUMERIC(9)
  , ins_datetime TIMESTAMP
  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , constraint c_grp_mst_PKC primary key (comp_cd,stre_cd,cls_grp_cd,mdl_smlcls_cd)
) ;

-- 郵便番号マスタ
drop table if exists c_zipcode_mst cascade;
drop sequence if exists c_zipcode_mst_s0;

create sequence c_zipcode_mst_s0 cycle;
create table c_zipcode_mst (
  serial_no NUMERIC(9) default nextval('c_zipcode_mst_s0')
  , post_no VARCHAR(7)
  , address1 VARCHAR(20)
  , address2 VARCHAR(20)
  , address3 VARCHAR(40)
  , kana_address1 VARCHAR(40)
  , kana_address2 VARCHAR(40)
  , kana_address3 VARCHAR(80)
  , ins_datetime TIMESTAMP
  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , constraint c_zipcode_mst_PKC primary key (serial_no)
) ;

-- 税金テーブルマスタ
drop table if exists c_tax_mst cascade;

create table c_tax_mst (
  comp_cd NUMERIC(9)
  , tax_cd SMALLINT
  , tax_name VARCHAR(5)
  , tax_typ SMALLINT default 0 not null
  , odd_flg SMALLINT default 0 not null
  , tax_per NUMERIC(5,2) default 0 not null
  , mov_cd SMALLINT default 0 not null
  , ins_datetime TIMESTAMP
  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , constraint c_tax_mst_PKC primary key (comp_cd,tax_cd)
) ;

-- カレンダーマスタ
drop table if exists c_caldr_mst cascade;

create table c_caldr_mst (
  comp_cd NUMERIC(9)
  , stre_cd NUMERIC(9)
  , caldr_date TIMESTAMP
  , dayoff_flg SMALLINT default 0 not null
  , comment1 VARCHAR(50)
  , comment2 VARCHAR(50)
  , am_weather SMALLINT default 0 not null
  , pm_weather SMALLINT default 0 not null
  , min_temp NUMERIC(5,2) default 0 not null
  , max_temp NUMERIC(5,2) default 0 not null
  , close_flg SMALLINT default 0 not null
  , open_rslt SMALLINT default 0 not null
  , close_rslt SMALLINT default 0 not null
  , rsrv_cust NUMERIC(8) default 0 not null
  , ins_datetime TIMESTAMP
  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , constraint c_caldr_mst_PKC primary key (comp_cd,stre_cd,caldr_date)
) ;

-- 祝日マスタ
drop table if exists s_nathldy_mst cascade;

create table s_nathldy_mst (
  caldr_date TIMESTAMP
  , comment1 VARCHAR(50)
  , ins_datetime TIMESTAMP
  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , constraint s_nathldy_mst_PKC primary key (caldr_date)
) ;


-- 02
-- 商品マスタ
drop table if exists c_plu_mst cascade;

create table c_plu_mst (
  comp_cd NUMERIC(9)
  , stre_cd NUMERIC(9)
  , plu_cd VARCHAR(20)
  , lrgcls_cd NUMERIC(9) default 0 not null
  , mdlcls_cd NUMERIC(9) default 0 not null
  , smlcls_cd NUMERIC(9) default 0 not null
  , tnycls_cd NUMERIC(9) default 0 not null
  , eos_cd VARCHAR(20)
  , bar_typ SMALLINT default 0 not null
  , item_typ SMALLINT default 0 not null
  , item_name VARCHAR(50)
  , pos_name VARCHAR(50)
  , list_typcapa VARCHAR(50)
  , list_prc NUMERIC(9) default 0 not null
  , instruct_prc NUMERIC(9) default 0 not null
  , pos_prc NUMERIC(9) default 0 not null
  , cust_prc NUMERIC(9) default 0 not null
  , cost_prc NUMERIC(11,2) default 0 not null
  , chk_amt NUMERIC(9) default 0 not null
  , tax_cd_1 SMALLINT default 0 not null
  , tax_cd_2 SMALLINT default 0 not null
  , tax_cd_3 SMALLINT default 0 not null
  , tax_cd_4 SMALLINT default 0 not null
  , cost_tax_cd SMALLINT default 0 not null
  , cost_per NUMERIC(5,2) default 0 not null
  , rbtpremium_per NUMERIC(5,2) default 1.00 not null
  , nonact_flg SMALLINT default 0 not null
  , prc_chg_flg SMALLINT default 0 not null
  , rbttarget_flg SMALLINT default 0 not null
  , stl_dsc_flg SMALLINT default 0 not null
  , weight_cnt SMALLINT default 0 not null
  , plu_tare SMALLINT default 0 not null
  , self_cnt_flg SMALLINT default 0 not null
  , guara_month SMALLINT default 0 not null
  , multprc_flg SMALLINT default 0 not null
  , multprc_per NUMERIC(5,2) default 0 not null
  , weight_flg SMALLINT default 0 not null
  , mbrdsc_flg SMALLINT default 0 not null
  , mbrdsc_prc NUMERIC(7) default 0 not null
  , mny_tckt_flg SMALLINT default 0 not null
  , stlplus_flg SMALLINT default 0 not null
  , prom_tckt_no NUMERIC(6) default 0 not null
  , weight NUMERIC(6) default 0 not null
  , pctr_tckt_flg SMALLINT default 0 not null
  , btl_prc NUMERIC(7) default 0 not null
  , clsdsc_flg SMALLINT default 0 not null
  , cpn_flg SMALLINT default 0 not null
  , cpn_prc NUMERIC(7) default 0 not null
  , plu_cd_flg SMALLINT default 0 not null
  , self_alert_flg SMALLINT default 0 not null
  , chg_ckt_flg SMALLINT default 0 not null
  , self_weight_flg SMALLINT default 0 not null
  , msg_name VARCHAR(50)
  , msg_flg SMALLINT default 0 not null
  , msg_name_cd NUMERIC(9) default 0 not null
  , pop_msg VARCHAR(50)
  , pop_msg_flg SMALLINT default 0 not null
  , pop_msg_cd NUMERIC(9) default 0 not null
  , liqrcls_cd NUMERIC(6) default 0 not null
  , liqr_typcapa NUMERIC(7) default 0 not null
  , alcohol_per NUMERIC(5,2) default 0 not null
  , liqrtax_cd NUMERIC(6) default 0 not null
  , use1_start_date TIMESTAMP
  , use2_start_date TIMESTAMP
  , prc_exe_flg SMALLINT default 0 not null
  , tmp_exe_flg SMALLINT default 0 not null
  , cust_dtl_flg SMALLINT default 0 not null
  , tax_exemption_flg SMALLINT default 0 not null
  , point_add NUMERIC(7) default 0 not null
  , coupon_flg SMALLINT default 0 not null
  , kitchen_prn_flg SMALLINT default 0 not null
  , pricing_flg SMALLINT default 0 not null
  , bc_tckt_cnt SMALLINT default 0 not null
  , last_sale_datetime TIMESTAMP
  , maker_cd NUMERIC(9) default 0 not null
  , user_val_1 NUMERIC(9) default 0 not null
  , user_val_2 NUMERIC(9) default 0 not null
  , user_val_3 NUMERIC(9) default 0 not null
  , user_val_4 NUMERIC(9) default 0 not null
  , user_val_5 NUMERIC(9) default 0 not null
  , user_val_6 VARCHAR(20)
  , prc_upd_system SMALLINT default 0 not null
  , ins_datetime TIMESTAMP
  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , cust_prc2 NUMERIC(9) default 0 not null
  , mbrdsc_prc2 NUMERIC(7) default 0 not null
  , producer_cd NUMERIC(9) default 0 not null
  , certificate_typ SMALLINT default 0 not null
  , kind_cd SMALLINT default 0 not null
  , div_cd SMALLINT default 0 not null
  , sub1_lrg_cd NUMERIC(9) default 0 not null
  , sub1_mdl_cd NUMERIC(9) default 0 not null
  , sub1_sml_cd NUMERIC(9) default 0 not null
  , sub2_lrg_cd NUMERIC(9) default 0 not null
  , sub2_mdl_cd NUMERIC(9) default 0 not null
  , sub2_sml_cd NUMERIC(9) default 0 not null
  , disc_cd NUMERIC(9) default 0 not null
  , typ_no VARCHAR(100)
  , dlug_flg SMALLINT default 0 not null
  , otc_flg SMALLINT default 0 not null
  , item_flg1 SMALLINT default 0 not null
  , item_flg2 SMALLINT default 0 not null
  , item_flg3 SMALLINT default 0 not null
  , item_flg4 SMALLINT default 0 not null
  , item_flg5 SMALLINT default 0 not null
  , item_flg6 SMALLINT default 0 not null
  , item_flg7 SMALLINT default 0 not null
  , item_flg8 SMALLINT default 0 not null
  , item_flg9 SMALLINT default 0 not null
  , item_flg10 SMALLINT default 0 not null
  , dpnt_rbttarget_flg SMALLINT default 0 not null
  , dpnt_usetarget_flg SMALLINT default 0 not null
  , constraint c_plu_mst_PKC primary key (comp_cd,stre_cd,plu_cd)
) ;

-- スキャニングＰＬＵテーブル
drop table if exists c_scanplu_mst cascade;

create table c_scanplu_mst (
  comp_cd NUMERIC(9)
  , stre_cd NUMERIC(9)
  , sku_cd VARCHAR(20)
  , plu_cd VARCHAR(20)
  , ins_datetime timestamp
  , upd_datetime timestamp
  , status smallint default 0 not null
  , send_flg smallint default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , constraint c_scanplu_mst_PKC primary key (comp_cd,stre_cd,sku_cd)
) ;

-- セット商品マスタ
drop table if exists c_setitem_mst cascade;

create table c_setitem_mst (
  comp_cd NUMERIC(9)
  , setplu_cd VARCHAR(20)
  , plu_cd VARCHAR(20)
  , item_qty NUMERIC(3) default 0 not null
  , ins_datetime TIMESTAMP
  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , constraint c_setitem_mst_PKC primary key (comp_cd,setplu_cd,plu_cd)
) ;

-- ケース商品マスタ
