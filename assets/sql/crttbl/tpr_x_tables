  , data16 NUMERIC(12) default 0 not null
  , data17 NUMERIC(12) default 0 not null
  , data18 NUMERIC(12) default 0 not null
  , data19 NUMERIC(12) default 0 not null
  , data20 NUMERIC(15,3) default 0 not null
  , data21 NUMERIC(14,2) default 0 not null
  , constraint rdly_tax_deal_PKC primary key (comp_cd,stre_cd,mac_no,chkr_no,cshr_no,sale_date,mode,kind,sub,func_cd)
) ;

