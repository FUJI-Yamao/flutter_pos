  , tel_no2 VARCHAR(25)
  , sex SMALLINT default 0 not null
  , cust_status SMALLINT default 0 not null
  , admission_date TIMESTAMP
  , withdraw_date TIMESTAMP
  , withdraw_typ NUMERIC(6) default 0 not null
  , withdraw_resn TEXT
  , card_clct_typ SMALLINT default 0 not null
  , custzone_cd NUMERIC(9) default 0 not null
  , post_no VARCHAR(7)
  , address1 VARCHAR(20)
  , address2 VARCHAR(20)
  , address3 VARCHAR(40)
  , mail_addr VARCHAR(50)
  , mail_flg SMALLINT default 0 not null
  , dm_flg SMALLINT default 0 not null
  , password VARCHAR(25)
  , targ_typ SMALLINT default 0 not null
  , attrib1 NUMERIC(6) default 0 not null
  , attrib2 NUMERIC(6) default 0 not null
  , attrib3 NUMERIC(6) default 0 not null
  , attrib4 NUMERIC(6) default 0 not null
  , attrib5 NUMERIC(6) default 0 not null
  , attrib6 NUMERIC(6) default 0 not null
  , attrib7 NUMERIC(6) default 0 not null
  , attrib8 NUMERIC(6) default 0 not null
  , attrib9 NUMERIC(6) default 0 not null
  , attrib10 NUMERIC(6) default 0 not null
  , mov_flg SMALLINT default 0 not null
  , pre_cust_no VARCHAR(20)
  , remark TEXT
  , ins_datetime TIMESTAMP
  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , svs_cls_cd NUMERIC(9) default 999999 not null
  , staff_flg SMALLINT default 0 not null
  , cust_prc_type SMALLINT default 0 not null
  , address4 VARCHAR(40)
  , tel_flg SMALLINT default 0 not null
  , constraint c_cust_mst_PKC primary key (cust_no,comp_cd)
) ;

