  , prn_upp_lim NUMERIC(9) default 0 not null
  , ref_typ SMALLINT default 0 not null
  , stp_acct_cd NUMERIC(9) default 0 not null
  , stp_red_amt NUMERIC(9) default 0 not null
  , sng_prn_flg SMALLINT default 0 not null
  , cust_kind_flg SMALLINT default 0 not null
  , timesch_flg SMALLINT default 0 not null
  , sun_flg SMALLINT default 0 not null
  , mon_flg SMALLINT default 0 not null
  , tue_flg SMALLINT default 0 not null
  , wed_flg SMALLINT default 0 not null
  , thu_flg SMALLINT default 0 not null
  , fri_flg SMALLINT default 0 not null
  , sat_flg SMALLINT default 0 not null
  , date_flg1 SMALLINT default 0 not null
  , date_flg2 SMALLINT default 0 not null
  , date_flg3 SMALLINT default 0 not null
  , date_flg4 SMALLINT default 0 not null
  , date_flg5 SMALLINT default 0 not null
  , stp_cpn_id NUMERIC(11)
  , cust_kind_trgt SMALLINT[]  NOT NULL  DEFAULT '{0}'
  , ref_typ2 SMALLINT default 0 not null
  , cust_card_kind SMALLINT default 0 not null
  , n_custom1 NUMERIC(9) default 0 not null
  , n_custom2 NUMERIC(9) default 0 not null
  , n_custom3 NUMERIC(9) default 0 not null
  , n_custom4 NUMERIC(9) default 0 not null
  , n_custom5 NUMERIC(9) default 0 not null
  , n_custom6 NUMERIC(9) default 0 not null
