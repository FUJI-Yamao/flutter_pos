-- 08
-- 顧客マスタ
drop table if exists c_cust_mst cascade;

create table c_cust_mst (
  cust_no VARCHAR(20)
  , comp_cd NUMERIC(9)
  , stre_cd NUMERIC(9) default 0 not null
  , last_name VARCHAR(50)
  , first_name VARCHAR(50)
  , kana_last_name VARCHAR(50)
  , kana_first_name VARCHAR(50)
  , birth_day TIMESTAMP
