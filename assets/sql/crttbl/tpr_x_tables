) ;

-- 産地・メーカーマスタ
drop table if exists c_maker_mst cascade;

create table c_maker_mst (
  comp_cd NUMERIC(9)
  , maker_cd NUMERIC(9)
  , parentmaker_cd NUMERIC(9)
  , name VARCHAR(25)
  , short_name VARCHAR(5)
  , kana_name VARCHAR(25)
  , ins_datetime TIMESTAMP
  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , constraint c_maker_mst_PKC primary key (comp_cd,maker_cd,parentmaker_cd)
) ;

-- 生産者品目マスタ
drop table if exists c_producer_mst cascade;

create table c_producer_mst (
  comp_cd NUMERIC(9)
  , stre_cd NUMERIC(9)
  , producer_cd NUMERIC(9)
  , name VARCHAR(25)
  , short_name VARCHAR(5)
  , ins_datetime TIMESTAMP
  , upd_datetime TIMESTAMP
  , status SMALLINT default 0 not null
  , send_flg SMALLINT default 0 not null
  , upd_user NUMERIC(10) default 0 not null
  , upd_system SMALLINT default 0 not null
  , tax_cd SMALLINT default 0 not null
  , producer_misc_1 SMALLINT default 0 not null
  , producer_misc_2 SMALLINT default 0 not null
  , producer_misc_3 SMALLINT default 0 not null
  , producer_misc_4 SMALLINT default 0 not null
  , producer_misc_5 SMALLINT default 0 not null
  , constraint c_producer_mst_PKC primary key (comp_cd,stre_cd,producer_cd)
) ;


-- 03
-- レジ情報マスタ
drop table if exists c_reginfo_mst cascade;

create table c_reginfo_mst (
  comp_cd NUMERIC(9)
  , stre_cd NUMERIC(9)
  , mac_no NUMERIC(9)
  , mac_typ SMALLINT default 0 not null
  , mac_addr VARCHAR(20)
  , ip_addr VARCHAR(50)
